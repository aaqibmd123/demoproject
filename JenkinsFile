pipeline {
    agent any

    qa {
        FEATURE_BRANCHES = ['fee', 'fe']
    }

    stages {
        stage('Delete Branches') {
            steps {
                script {
                    FEATURE_BRANCHES.each { branch ->
                        def gitDeleteBranch = """
                            git push origin --delete ${branch}
                        """
                        sh gitDeleteBranch
                    }
                }
            }
        }
    }
}
